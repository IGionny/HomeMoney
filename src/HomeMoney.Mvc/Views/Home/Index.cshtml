@using System.Security.Claims
@using HomeMoney.Mvc.Models
@using Newtonsoft.Json

@{
  var userContext = new UserContext(User.Identity.Name, User.Claims.FirstOrDefault(x => x.Type == ClaimTypes.Email)?.Value);
}
<template id="UserContext">@Html.Raw(JsonConvert.SerializeObject(userContext))</template>

<div id="App">
</div>

@section Scripts{
  <script src="/VueJS/App.js"></script>
}